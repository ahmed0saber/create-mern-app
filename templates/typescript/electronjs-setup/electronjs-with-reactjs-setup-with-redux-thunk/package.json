{
  "name": "project_name",
  "version": "0.1.0",
  "description": "Simple boilerplate for developing an Electron app with React",
  "main": "src/electron.js",
  "private": true,
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "eject-react": "react-scripts eject",
    "build-electron": "electron-builder",
    "build": "npm run build-react && npm run build-electron -wml",
    "dev": "concurrently \"cross-env BROWSER=none npm run start-react\" \"wait-on http://localhost:3000 && nodemon --exec electron .\""
  },
  "author": "author_email",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-open-weather": "^1.1.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "nodemon": "^2.0.12",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "react.electron",
    "productName": "react-electron",
    "mac": {
      "icon": "build/logo512.png",
      "target": "tar.gz"
    },
    "linux": {
      "category": "Utils",
      "icon": "build/icon.png",
      "packageCategory": "utils",
      "maintainer": "author_email",
      "asar": true,
      "target": [
        "deb",
        "tar.xz"
      ],
      "synopsis": "Basically same as description but shorter"
    },
    "win": {
      "icon": "build/icon.png",
      "target": "nsis"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
